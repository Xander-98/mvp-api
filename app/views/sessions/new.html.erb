<h1>Login</h1>
<%= form_with method: "post", url: "/sessions" do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Submit" %>
  </div>
<% end%>